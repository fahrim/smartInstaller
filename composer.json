{
    "name": "smarteknoloji/smart-installer",
    "description": "Smart installer for Laravel",
    "license": "GPL-3.0-or-later",
    "authors": [
        {
            "name": "Fahri Meral",
            "email": "fahri.meral@smarteknoloji.com",
            "homepage": "https://www.smarteknoloji.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "Smarteknoloji\\SmartlInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Smarteknoloji\\SmartlInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}