{
    "name": "fahrim/smart-installer",
    "description": "Smart installer for Laravel",
    "authors": [
        {
            "name": "Fahri Meral",
            "email": "fahri.meral@smarteknoloji.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "RachidLaasri\\LaravelInstaller\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RachidLaasri\\LaravelInstaller\\Providers\\LaravelInstallerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}